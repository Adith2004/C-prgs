// C program using Struct initialization

#include<stdio.h>
#include<string.h>

struct student {
	int roll;
	float cgpa;
	char name[100];
};

int main(){
	struct student s1 = {1664, 9.2, "Ronaldo"};
	
	printf("Student name :%s\n", s1.name);
	printf("Student cgpa :%f\n", s1.cgpa);
	printf("Student Roll number :%d\n", s1.roll);
	
	return 0;	
}
