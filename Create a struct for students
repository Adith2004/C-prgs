// Create a struct for students

#include<stdio.h>
#include<string.h>

struct student {
	int roll;
	float cgpa;
	char name[100];
};

int main(){
	struct student s1;
	s1.roll = 1664;
	s1.cgpa = 9.2;
	strcpy(s1.name, "Ronaldo");
	
	printf("Student name :%s\n", s1.name);
	printf("Student cgpa :%f\n", s1.cgpa);
	printf("Student Roll number :%d\n", s1.roll);
	
	struct student s2;
	s2.roll = 1661;
	s2.cgpa = 9.1;
	strcpy(s2.name, "Messi");
	
	printf("Student name :%s\n", s2.name);
	printf("Student cgpa :%f\n", s2.cgpa);
	printf("Student Roll number :%d\n", s2.roll);
	
	struct student s3;
	s3.roll = 1662;
	s3.cgpa = 9.0;
	strcpy(s3.name, "Neymar");
	
	printf("Student name :%s\n", s3.name);
	printf("Student cgpa :%f\n", s3.cgpa);
	printf("Student Roll number :%d\n", s3.roll);
	
	return 0;	
}
